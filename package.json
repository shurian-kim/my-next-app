{
  "name": "my-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f ./env/.env.local next dev",
    "build": "env-cmd -f ./env/.env.local next build && next export",
    "start": "env-cmd -f ./env/.env.local next start",
    "build:dev": "env-cmd -f ./env/.env.development next build",
    "start:dev": "env-cmd -f ./env/.env.development next start",
    "build:prod": "env-cmd -f ./env/.env.production next build",
    "start:prod": "env-cmd -f ./env/.env.production next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.3.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@reduxjs/toolkit": "^1.8.6",
    "@types/axios": "^0.14.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/react-query": "^1.2.9",
    "@types/recoil": "^0.0.9",
    "env-cmd": "^10.1.0",
    "framer-motion": "^6",
    "next": "13.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.8.4"
  }
}
